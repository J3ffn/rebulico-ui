{
  "name": "rebulico-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "prod": "set NODE_ENV=production && vite --mode production",
    "dev": "set NODE_ENV=development && vite --mode development",
    "local": "set NODE_ENV=local && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
